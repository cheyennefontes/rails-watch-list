<h2 class="m-5 p-2">All Lists</h2>
<div class="cards container m-5 p-2 ">
  <% @lists.each do |list| %>
  <div class="card-product">
  <div class="card-product-infos">
   <h1><%= link_to list.name, list_path(list), class:'text-decoration-none' %></h1>
     <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('<%= cl_image_path list.photo.key, :cloud_name => "dcfii4sfc" if list.photo.attached? %>')">
    <h2> <%= list.name %> </h2>
  </div>
  </div>
  </div>
    <% end %>
  </div>
